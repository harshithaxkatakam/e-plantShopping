{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "build" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "homepage": "https://harshithaxkatakam.github.io/e-plantShopping",
    "scripts": {
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build"
    }
} 